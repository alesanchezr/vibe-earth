{
  "name": "geek-world",
  "version": "1.0.0",
  "description": "3D visualization of geeks coming online on a spherical planet",
  "scripts": {
    "start": "cd src/backend && npm start",
    "dev": "cd src/backend && npm run dev",
    "frontend": "vite",
    "frontend:build": "vite build",
    "frontend:preview": "vite preview",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend\"",
    "migrate": "cd src/backend && npm run migrate",
    "migrate:up": "cd src/backend && npm run migrate:up",
    "migrate:down": "cd src/backend && npm run migrate:down",
    "setup": "npm install && cd src/backend && npm install"
  },
  "keywords": [
    "threejs",
    "websocket",
    "visualization",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@tweenjs/tween.js": "^25.0.0",
    "three": "^0.174.0",
    "uber-noise": "^0.2.2"
  }
}
